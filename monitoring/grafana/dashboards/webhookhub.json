{
  "dashboard": {
    "title": "WebHookHub Metrics",
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(webhookhub_requests_total[5m])",
            "legendFormat": "{{method}}"
          }
        ]
      },
      {
        "id": 2,
        "title": "Active Endpoints",
        "type": "stat",
        "targets": [
          {
            "expr": "webhookhub_endpoints_total"
          }
        ]
      },
      {
        "id": 3,
        "title": "Total Webhooks Received",
        "type": "stat",
        "targets": [
          {
            "expr": "webhookhub_requests_total"
          }
        ]
      }
    ]
  }
}