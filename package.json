{
  "name": "webhookhub",
  "version": "1.0.0",
  "description": "Webhook testing and monitoring platform",
  "type": "module",
  "main": "src/server/index.js",
  "scripts": {
    "start": "node src/server/index.js",
    "server": "node --watch src/server/index.js",
    "client": "cd frontend && npm run dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd frontend && npm run build",
    "test": "node --test src/server/*.test.js"
  },
  "keywords": [
    "webhook",
    "devops",
    "monitoring"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@hono/node-server": "^1.19.9",
    "@tanstack/react-router": "^1.157.16",
    "concurrently": "^9.2.1",
    "dotenv": "^16.4.5",
    "framer-motion": "^12.29.2",
    "hono": "^4.5.0",
    "lucide-react": "^0.563.0",
    "nanoid": "^5.0.7",
    "postgres": "^3.4.4",
    "prom-client": "^15.1.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "recharts": "^3.7.0",
    "redis": "^4.6.14",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.8.3",
    "winston": "^3.19.0",
    "winston-elasticsearch": "^0.17.4"
  }
}
